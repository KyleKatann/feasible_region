    "%matplotlib inline\n",
    "import feasible_region\n",
    "import sympy as sym\n",
    "x,y=sym.symbols(\"x y\")\n",
    "u=feasible_region.model()\n",
    "u.spacelimit(0,5,0,5,0.01)\n",
    "u.addConstr(y+x>=3)\n",
    "u.addConstr(x>=1)\n",
    "u.addConstr(y-4 < 0)\n",
    "u.show()"
   ]
  }
